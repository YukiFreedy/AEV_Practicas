<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="Practica2" />
		<Option makefile_is_custom="1" />
		<Option execution_dir="${PROJECT_DIR}" />
		<Option pch_mode="2" />
		<Option compiler="cb3ds" />
		<Build>
			<Target title="Debug">
				<Option output="Practica2" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Debug/" />
				<Option type="1" />
				<Option compiler="cb3ds" />
				<Compiler>
					<Add option="-g" />
				</Compiler>
				<ExtraCommands>
					<Add after="desmume $(PROJECT_DIR)$(PROJECT_NAME).nds" />
					<Mode after="always" />
				</ExtraCommands>
				<MakeCommands>
					<Build command="$make -f $makefile" />
					<CompileFile command="$make -f $makefile $file" />
					<Clean command="$make -f $makefile clean" />
					<DistClean command="$make -f $makefile distclean$target" />
					<AskRebuildNeeded command="$make -q -f $makefile" />
					<SilentBuild command="$make -f $makefile &gt; $(CMD_NULL)" />
				</MakeCommands>
			</Target>
			<Target title="Release">
				<Option output="Practica2" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Release/" />
				<Option type="1" />
				<Option compiler="cb3ds" />
				<Compiler>
					<Add option="-O2" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
		</Compiler>
		<Unit filename="build/Multilayer.d" />
		<Unit filename="build/RotBackgrounds.d" />
		<Unit filename="build/TextBackgrounds.d" />
		<Unit filename="build/advanced.d" />
		<Unit filename="build/basic.d" />
		<Unit filename="build/handmade.d" />
		<Unit filename="build/main.d" />
		<Unit filename="build/scrolling.d" />
		<Unit filename="include/Multilayer.h" />
		<Unit filename="include/RotBackgrounds.h" />
		<Unit filename="include/TextBackgrounds.h" />
		<Unit filename="source/Multilayer.s" />
		<Unit filename="source/RotBackgrounds.s" />
		<Unit filename="source/TextBackgrounds.s" />
		<Unit filename="source/advanced.cpp" />
		<Unit filename="source/basic.cpp" />
		<Unit filename="source/functions.cpp" />
		<Unit filename="source/handmade.cpp" />
		<Unit filename="source/main.cpp" />
		<Unit filename="source/scrolling.cpp" />
		<Extensions>
			<envvars />
			<code_completion />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
